import json

def generate_question():
    question = "다음 중 인공지능 알고리즘에 기반한 신용평가시스템에서 정보주체가 평가결과에 대해 가질 수 있는 권리를 규정한 조항은?"
    options = {
        "1": "제36조의2",
        "2": "제37조",
        "3": "제38조",
        "4": "제39조"
    }
    answer_key = "1"

    return {
        "question": question,
        "options": options,
        "answer": answer_key
    }

