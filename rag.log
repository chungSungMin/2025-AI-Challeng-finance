import re

def remove_punctuation(text):
    pattern = r'[^\w\s]'   # 단어문자와 공백을 제외한 모든 문자 매칭
    cleaned_text = re.sub(pattern, '', text)
    
    paragraphs = text.split('\n')
    processed_paragraphs = []
    for paragraph in paragraphs:
        lines = paragraph.strip().split('.')
        new_lines = [''.join(re.findall(r'\S+', l)) for l in lines]
        processed_paragraphs.append('.'.join(new_lines))
        
    result = '\n\n'.join(processed_paragraphs).strip()
    return result

# 사용 예
sample_text = """Hello!
This is a sample text with punctuation marks at the end of each sentence.
Each line ends with a period (.), comma (,), or exclamation point (!).
Another paragraph starts here."""
print(remove_punctuation(sample_text))
